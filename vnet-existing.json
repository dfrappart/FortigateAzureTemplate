    "subnet2ID": {
      "value": "[variables('subnet2ID')]",
      "type": "string"
    },
    "subnet3ID": {
      "value": "[variables('subnet3ID')]",
      "type": "string"
    },
    "subnet4ID": {
      "value": "[variables('subnet4ID')]",
      "type": "string"
    },
    "subnet5ID": {
      "value": "[variables('subnet5ID')]",
      "type": "string"
    },
    "subnet6ID": {
      "value": "[variables('subnet6ID')]",
      "type": "string"
    },
    "subnet7ID": {
      "value": "[variables('subnet7ID')]",
