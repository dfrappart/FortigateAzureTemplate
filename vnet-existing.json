{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "type": "string",
      "metadata": {
        "description": "Location"
      }
    },
    "vnetName": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network name (e.g. vnet-haivision)"
      }
    },
    "vnetResourceGroup": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network Resource Group"
      }
    },
    "vnetAddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network Address Prefix (i.e. CIDR)"
      }
    },
    "OutsideSubnetName": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet1AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "DMZSubnetName": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet2AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "InternalSubnetName": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet3AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "Subnet4Name": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet4AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "Subnet5Name": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet5AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "Subnet6Name": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet6AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "Subnet7Name": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet7AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "Subnet8Name": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet8AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "routeTable1Name": {
      "type": "string"
    },
    "routeTable2Name": {
      "type": "string"
    },
    "routeTable3Name": {
      "type": "string"
    },
    "routeTable4Name": {
      "type": "string"
    },
    "routeTable5Name": {
      "type": "string"
    },
    "routeTable6Name": {
      "type": "string"
    },
    "routeTable7Name": {
      "type": "string"
    },
    "routeTable8Name": {
      "type": "string"
    }
  },
  "variables": {
    "vnetID": "[resourceId(parameters('vnetResourceGroup'), 'Microsoft.Network/virtualNetworks', parameters('vnetName'))]",
    "subnet1ID": "[concat(variables('vnetID'),'/subnets/',parameters('OutsideSubnetName'))]",
    "subnet2ID": "[concat(variables('vnetID'),'/subnets/',parameters('DMZSubnetName'))]",
    "subnet3ID": "[concat(variables('vnetID'),'/subnets/',parameters('InternalSubnetName'))]",
    "subnet4ID": "[concat(variables('vnetID'),'/subnets/',parameters('Subnet4Name'))]",
    "subnet5ID": "[concat(variables('vnetID'),'/subnets/',parameters('Subnet5Name'))]",
    "subnet6ID": "[concat(variables('vnetID'),'/subnets/',parameters('Subnet6Name'))]",
    "subnet7ID": "[concat(variables('vnetID'),'/subnets/',parameters('Subnet7Name'))]",
    "subnet8ID": "[concat(variables('vnetID'),'/subnets/',parameters('Subnet8Name'))]",
    "routeTable1Id": "[resourceId('Microsoft.Network/routeTables',parameters('routeTable1Name'))]",
    "routeTable2Id": "[resourceId('Microsoft.Network/routeTables',parameters('routeTable2Name'))]",
    "routeTable3Id": "[resourceId('Microsoft.Network/routeTables',parameters('routeTable3Name'))]",
