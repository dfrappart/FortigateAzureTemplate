{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "type": "string",
      "metadata": {
        "description": "Location"
      }
    },
    "vnetName": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network name (e.g. vnet-haivision)"
      }
    },
    "vnetResourceGroup": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network Resource Group"
      }
    },
    "vnetAddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network Address Prefix (i.e. CIDR)"
      }
    },
    "subnet1Name": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet1AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "subnet2Name": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet2AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "subnet3Name": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet3AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "subnet4Name": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet4AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "subnet5Name": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet5AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "subnet6Name": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet6AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "subnet7Name": {
      "type": "string",
      "metadata": {
        "description": "Server will be deployed into this subnet within the Virtual Network"
      }
    },
    "subnet7AddressPrefix": {
      "type": "string",
      "metadata": {
        "description": "Virtual Network subnet Address Prefix (i.e. CIDR)"
      }
    },
    "routeTable1Name": {
      "type": "string"
    },
    "routeTable2Name": {
      "type": "string"
    },
    "routeTable3Name": {
      "type": "string"
    },
    "routeTable4Name": {
      "type": "string"
    },
    "routeTable5Name": {
      "type": "string"
    },
    "routeTable6Name": {
      "type": "string"
    },
    "routeTable7Name": {
      "type": "string"
    }
  },
  "variables": {
    "vnetID": "[resourceId(parameters('vnetResourceGroup'), 'Microsoft.Network/virtualNetworks', parameters('vnetName'))]",
    "subnet1ID": "[concat(variables('vnetID'),'/subnets/',parameters('subnet1Name'))]",
    "subnet2ID": "[concat(variables('vnetID'),'/subnets/',parameters('subnet2Name'))]",
    "subnet3ID": "[concat(variables('vnetID'),'/subnets/',parameters('subnet3Name'))]",
    "subnet4ID": "[concat(variables('vnetID'),'/subnets/',parameters('subnet4Name'))]",
    "subnet5ID": "[concat(variables('vnetID'),'/subnets/',parameters('subnet5Name'))]",
    "subnet6ID": "[concat(variables('vnetID'),'/subnets/',parameters('subnet6Name'))]",
    "subnet7ID": "[concat(variables('vnetID'),'/subnets/',parameters('subnet7Name'))]",
    "routeTable1Id": "[resourceId('Microsoft.Network/routeTables',parameters('routeTable1Name'))]",
    "routeTable2Id": "[resourceId('Microsoft.Network/routeTables',parameters('routeTable2Name'))]",
    "routeTable3Id": "[resourceId('Microsoft.Network/routeTables',parameters('routeTable3Name'))]",
    "routeTable4Id": "[resourceId('Microsoft.Network/routeTables',parameters('routeTable4Name'))]",
    "routeTable5Id": "[resourceId('Microsoft.Network/routeTables',parameters('routeTable5Name'))]",
    "routeTable6Id": "[resourceId('Microsoft.Network/routeTables',parameters('routeTable6Name'))]",
    "routeTable7Id": "[resourceId('Microsoft.Network/routeTables',parameters('routeTable7Name'))]",
    "apiVer": "2015-06-15"
  },
  "resources": [
    {
      "apiVersion": "[variables('apiVer')]",
      "type": "Microsoft.Network/virtualNetworks",
      "name": "[parameters('vnetName')]",
      "location": "[parameters('location')]",
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[parameters('vnetAddressPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[parameters('subnet1Name')]",
            "properties": {
              "addressPrefix": "[parameters('subnet1AddressPrefix')]",
              "routeTable": {
                "id": "[variables('routeTable1Id')]"
              }
            }
          },
          {
            "name": "[parameters('Subnet2Name')]",
            "properties": {
              "addressPrefix": "[parameters('Subnet2AddressPrefix')]",
              "routeTable": {
                "id": "[variables('routeTable2Id')]"
              }
            }
          },
          {
            "name": "[parameters('Subnet3Name')]",
            "properties": {
              "addressPrefix": "[parameters('Subnet3AddressPrefix')]",
              "routeTable": {
                "id": "[variables('routeTable3Id')]"
              }
            }
          },
          {
            "name": "[parameters('Subnet4Name')]",
            "properties": {
              "addressPrefix": "[parameters('Subnet4AddressPrefix')]",
              "routeTable": {
                "id": "[variables('routeTable4Id')]"
              }
            }
          },
          {
            "name": "[parameters('Subnet5Name')]",
            "properties": {
              "addressPrefix": "[parameters('Subnet5AddressPrefix')]",
              "routeTable": {
                "id": "[variables('routeTable5Id')]"
              }
            }
          },
          {
            "name": "[parameters('Subnet6Name')]",
            "properties": {
              "addressPrefix": "[parameters('Subnet6AddressPrefix')]",
              "routeTable": {
                "id": "[variables('routeTable6Id')]"
              }
            }
          },
          {
            "name": "[parameters('Subnet7Name')]",
            "properties": {
              "addressPrefix": "[parameters('Subnet7AddressPrefix')]",
              "routeTable": {
                "id": "[variables('routeTable7Id')]"
              }
            }
          }
        ]
      }
    }
  ],
  "outputs": {
    "subnet1ID": {
      "value": "[variables('subnet1ID')]",
      "type": "string"
    },
    "subnet2ID": {
      "value": "[variables('subnet2ID')]",
      "type": "string"
    },
     "subnet3ID": {
      "value": "[variables('subnet3ID')]",
      "type": "string"
    },
     "subnet4ID": {
      "value": "[variables('subnet4ID')]",
      "type": "string"
    },
     "subnet5ID": {
      "value": "[variables('subnet5ID')]",
      "type": "string"
    },
     "subnet6ID": {
      "value": "[variables('subnet6ID')]",
      "type": "string"
    },
     "subnet7ID": {
      "value": "[variables('subnet7ID')]",
      "type": "string"
    }
  }
}
